{
	"workbench.editor.enablePreview": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.editor.showIcons": false,
	"workbench.editor.showTabs": false,
	"workbench.editor.tabCloseButton": "off",
	"workbench.panel.defaultLocation": "right",
	"workbench.settings.editor": "json",
	"workbench.sideBar.location": "left",
	"workbench.list.automaticKeyboardNavigation": false,
	"workbench.startupEditor": "none",
	"workbench.statusBar.visible": true,
	"workbench.tips.enabled": false,
	"workbench.tree.indent": 12,
	"workbench.tree.renderIndentGuides": "none",
	"editor.cursorBlinking": "phase",
	"editor.cursorSmoothCaretAnimation": false,
	"editor.cursorStyle": "line",
	"editor.fontFamily": "Cascadia Mono, Bedstead, Input Mono, SF Mono, Fira Code, Victor Mono, iA Writer Duo V, ALS Hauss Mono VF, Inconsolata",
	"editor.fontLigatures": true,
	"editor.fontWeight": "500",
	"editor.fontSize": 18,
	"editor.lineHeight": 40,
	"editor.minimap.enabled": false,
	"editor.minimap.renderCharacters": false,
	"editor.multiCursorModifier": "alt",
	"editor.renderIndentGuides": false,
	"editor.tabCompletion": "on",
	"editor.wordBasedSuggestions": false,
	"editor.wordWrap": "off",
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.snippetSuggestions": "top",
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.overviewRulerBorder": false,
	"editor.renderLineHighlight": "all",
	"terminal.integrated.fontFamily": "Fira Code",
	"terminal.integrated.fontSize": 14,
	"zenMode.fullScreen": false,
	"zenMode.centerLayout": true,
	"zenMode.hideLineNumbers": false,
	"window.titleBarStyle": "custom",
	"window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
	"explorer.openEditors.visible": 0,
	"explorer.confirmDragAndDrop": false,
	"terminal.integrated.cursorStyle": "line",
	"files.autoSave": "onFocusChange",
	"files.associations": {
		".babelrc": "json5",
		"*.js": "javascriptreact",
		"*.tpl": "html"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"**/.vscode": true,
		"**/.stencil": true,
		"**/__pycache__": true,
		"**/.pytest_cache": true
		// "**/node_modules": true
		// "**/build": true
		// "**/dist": true
	},
	"files.watcherExclude": {
		"**/build": true,
		"**/dist": true,
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true
	},
	"search.exclude": {
		".vscode/": true,
		"**/.next/": true,
		"**/node_modules": true,
		"**/bower_components": true
		// "**/build": true,
		// "**/dist": true
	},
	"git.confirmSync": false,
	"git.autofetch": true,
	"emmet.triggerExpansionOnTab": true,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact"
	},
	"emmet.syntaxProfiles": {
		"javascript": "jsx",
		"jsx": {
			"self_closing_tag": true
		}
	},
	"html.format.indentHandlebars": true,
	"javascript.updateImportsOnFileMove.enabled": "never",
	"javascript.validate.enable": false,
	"breadcrumbs.enabled": true,
	"extensions.ignoreRecommendations": true,
	"telemetry.enableCrashReporter": true,
	"telemetry.enableTelemetry": true,
	"workbench.colorTheme": "GitHub Light",
	"workbench.iconTheme": "vscode-icons-mac",
	"terminal.integrated.lineHeight": 1.4,
	"editor.suggestSelection": "first",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.renderWhitespace": "none",
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"prettier.tabWidth": 2,
	"prettier.singleQuote": true,
	"elmLS.elmFormatPath": "~/.npm-global/bin/elm-format",
	"editor.cursorSurroundingLines": 3,
	"editor.lineNumbers": "on",
	"window.autoDetectColorScheme": true,
	"workbench.preferredLightColorTheme": "GitHub Light",
	"workbench.preferredDarkColorTheme": "GitHub Dark",
	"presentationMode.zoomLevel": 2,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"[markdown]": {
		"files.trimTrailingWhitespace": false,
		"editor.formatOnSave": false
	},
	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.easymotionDimBackground": false,
	"vim.easymotionMarkerWidthPerChar": 24,
	"vim.easymotionMarkerBackgroundColor": "#d73a49",
	"vim.cursorStylePerMode.normal": "underline",
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<leader>", "d"],
			"after": ["d", "d"]
		},
		{
			"before": ["<C-n>"],
			"commands": [":nohl"]
		}
	],
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false
	},
	"todo-tree.general.tagGroups": {
		"FIXME": ["FIX", "FIXME", "FIXIT"]
	},
	"todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^)\\s*(@ts-expect-error ts-migrate(.*))?($TAGS)|^\\s*- \\[ \\])",
	"vetur.experimental.templateInterpolationService": true,
	"emmet.showSuggestionsAsSnippets": true,
	"workbench.productIconTheme": "fluent-icons",
	"workbench.colorCustomizations": {
		"editorGroup.border": "#ff000000"
	},
	"git.enableCommitSigning": true,
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": false,
		"markdown": false
	},
	"editor.bracketPairColorization.enabled": true,
	"editor.suggest.preview": true
}
