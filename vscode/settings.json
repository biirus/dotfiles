{
  "workbench.activityBar.visible": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.showIcons": false,
  "workbench.editor.showTabs": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.keyboardNavigation": "simple",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "none",
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": false,
  "editor.cursorStyle": "line",
  "editor.fontFamily": "iA Writer Duo V, Inconsolata, Input Mono, Hasklig, Cascadia Mono, SF Mono, Fira Code, Nitti Grotesk",
  "editor.fontLigatures": true,
  "editor.fontWeight": "500",
  "editor.fontSize": 20,
  "editor.lineHeight": 44,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.multiCursorModifier": "alt",
  "editor.renderIndentGuides": false,
  "editor.tabCompletion": "on",
  "editor.wordBasedSuggestions": false,
  "editor.wordWrap": "off",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.snippetSuggestions": "top",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 14,
  "zenMode.fullScreen": false,
  "zenMode.centerLayout": false,
  "zenMode.hideLineNumbers": true,
  "window.titleBarStyle": "native",
  "window.zoomLevel": 0,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "explorer.openEditors.visible": 0,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.cursorStyle": "line",
  "files.autoSave": "onFocusChange",
  "files.associations": {
    ".babelrc": "json5",
    "*.js": "javascriptreact",
    "*.tpl": "html"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/.stencil": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true
    // "**/node_modules": true,
    // "**/build": true,
    // "**/dist": true,
  },
  "files.watcherExclude": {
    "**/build": true,
    "**/dist": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  },
  "search.exclude": {
    ".vscode/": true,
    "**/.next/": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/build": true,
    "**/dist": true
  },
  "git.confirmSync": false,
  "git.autofetch": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "jsx-attr": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "jsx": {
      "self_closing_tag": true
    }
  },
  "html.format.indentHandlebars": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "javascript.validate.enable": false,
  "breadcrumbs.enabled": false,
  "extensions.ignoreRecommendations": true,
  "telemetry.enableCrashReporter": true,
  "telemetry.enableTelemetry": true,
  "workbench.colorTheme": "Nord",
  "workbench.iconTheme": "ayu",
  "terminal.integrated.lineHeight": 1.4,
  "editor.suggestSelection": "first",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderWhitespace": "all",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "elmLS.elmFormatPath": "~/.npm-global/bin/elm-format",
  "editor.cursorSurroundingLines": 3,
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "Ayu Light",
  "workbench.preferredDarkColorTheme": "Nord",
  "presentationMode.zoomLevel": 2,
  "vetur.experimental.templateInterpolationService": true,
  "typescript.updateImportsOnFileMove.enabled": "always"
}
